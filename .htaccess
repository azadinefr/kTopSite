<IfModule mod_rewrite.c>

	Options +FollowSymLinks -Indexes
	Options -MultiViews
	RewriteEngine On

	RewriteBase /kowframework/

	RedirectMatch 403 ^/(kow_system/.+|\.git).*$

	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !-f

	RewriteRule ^(.*)$ index.php?p=$1 [L]

</IfModule>
